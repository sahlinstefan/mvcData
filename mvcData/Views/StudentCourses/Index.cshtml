@model IEnumerable<mvcData.Models.StudentCourse>


<h2>Assignments and Students</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Courses.Course)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Students.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Courses.Course)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Students.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StudCourse_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.StudCourse_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StudCourse_Id })
        </td>
    </tr>
}

</table>
